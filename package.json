{
    "name": "@slimio/oop",
    "version": "2.0.0",
    "description": "JavaScript OOP library",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "test": "cross-env psp && jest --coverage",
        "coverage": "codecov"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/oop.git"
    },
    "keywords": [
        "SlimIO",
        "oop"
    ],
    "files": [
        "index.js",
        "index.d.ts",
        "src/utils.js"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "jest": {
        "collectCoverageFrom": [
            "index.js",
            "**/src/**/*.js"
        ],
        "testEnvironment": "node",
        "testMatch": [
            "**/test/**/*.js"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/test/fixtures/"
        ]
    },
    "bugs": {
        "url": "https://github.com/SlimIO/oop/issues"
    },
    "homepage": "https://github.com/SlimIO/oop#readme",
    "dependencies": {},
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^12.1.1",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^4.1.0",
        "@slimio/psp": "^0.12.3",
        "codecov": "^3.8.1",
        "cross-env": "^7.0.3",
        "eslint": "^7.18.0",
        "husky": "^4.3.8",
        "jest": "^26.6.3",
        "jsdoc": "^3.6.6",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=14"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
